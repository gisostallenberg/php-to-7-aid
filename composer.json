{
    "name": "gisostallenberg/php-to-7-aid",
    "description": "Tool to find and possibly fix backward incompatible changes in your existing codebase when upgrading to or preparing for PHP7",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Giso Stallenberg",
            "email": "gisostallenberg@gmail.com"
        }
    ],
    "keywords": "php7, php-7, upgrade, code fixer",
    "minimum-stability": "stable",
    "require": {
        "php": "^5.3.6||7.0.*",
        "symfony/console": "^2.7",
        "symfony/finder": "^2.7"
    },
    "require-dev": {
        "kherge/box": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "GisoStallenberg\\phpTo7aid\\": "src/"
        }
    },
    "bin": ["php-to-7-aid"]
}
